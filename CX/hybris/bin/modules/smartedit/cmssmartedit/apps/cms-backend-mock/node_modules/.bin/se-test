#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/bin/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/node_modules:/home/acxias/Documents/CX/hybris/bin/node_modules:/home/acxias/Documents/CX/hybris/node_modules:/home/acxias/Documents/CX/node_modules:/home/acxias/Documents/node_modules:/home/acxias/node_modules:/home/node_modules:/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules/smartedit-build/bin/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules/smartedit-build/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/bin/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/smartedit-build/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/apps/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/smartedittools/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/node_modules:/home/acxias/Documents/CX/hybris/bin/node_modules:/home/acxias/Documents/CX/hybris/node_modules:/home/acxias/Documents/CX/node_modules:/home/acxias/Documents/node_modules:/home/acxias/node_modules:/home/node_modules:/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules/smartedit-build/bin/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules/smartedit-build/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/cms-backend-mock/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/apps/node_modules:/home/acxias/Documents/CX/hybris/bin/modules/smartedit/cmssmartedit/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --inspect "$basedir/../smartedit-build/bin/index.js" "$@"
else
  exec node  --inspect "$basedir/../smartedit-build/bin/index.js" "$@"
fi
