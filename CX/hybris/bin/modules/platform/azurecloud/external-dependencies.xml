<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.hybris.platform</groupId>
	<artifactId>azurecloud</artifactId>
	<version>2205.34</version>

	<packaging>jar</packaging>

	<properties>
		<azure.storage.blob.version>12.26.0</azure.storage.blob.version>
		<azure.core.version>1.49.0</azure.core.version>
		<azure.json.version>1.1.0</azure.json.version>
		<azure.xml.version>1.0.0</azure.xml.version>
		<!-- the jackson version should be in sync with the version of jackson libraries from core extension -->
		<jackson.version>2.16.1</jackson.version>
		<azure.core.http.netty.version>1.15.0</azure.core.http.netty.version>
		<!-- the netty version should be in sync with the version of netty libraries from core extension -->
		<netty.version>4.1.108.Final</netty.version>
		<netty.tcnative.version>2.0.65.Final</netty.tcnative.version>
		<!-- the reactor.netty.version should be compatible with the version of reactor libraries from core extension -->
		<reactor.netty.version>1.0.43</reactor.netty.version>
		<azure.storage.common.version>12.25.0</azure.storage.common.version>
		<azure.storage.internal.avro.version>12.11.0</azure.storage.internal.avro.version>
	</properties>

